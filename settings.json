{
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "quarto"
    ],
    "spellright.ignoreRegExps": [
        "/&[A-Za-z]*;/g",
        "/@[a-z]*[0-9]{2,4}/g",
        "/[@#](?:fig|tbl)[-A-Za-z0-9]*/"
    ]
}